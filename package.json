{
  "name": "authtest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test": "pnpm test:integration && pnpm test:unit",
    "test-dev": "cross-env DATABASE_URL='file:test.db' prisma migrate deploy && cross-env DATABASE_URL='file:test.db' vite dev --port 4173",
    "test-preview": "cross-env DATABASE_URL='file:test.db' prisma migrate deploy && cross-env DATABASE_URL='file:test.db' vite build && cross-env DATABASE_URL='file:test.db' vite preview --port 4173",
    "test:integration": "cross-env DATABASE_URL='file:test.db' playwright test",
    "test:unit": "cross-env DATABASE_URL='file:test.db' vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.27.6",
    "@tailwindcss/typography": "^0.5.10",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "daisyui": "^4.4.14",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "prisma": "^5.6.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.2",
    "sveltekit-flash-message": "^2.2.2",
    "sveltekit-superforms": "^1.11.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "vite": "^4.5.0",
    "vitest": "^0.32.4",
    "zod": "^3.22.4"
  },
  "type": "module",
  "dependencies": {
    "@lucia-auth/adapter-prisma": "^3.0.2",
    "@prisma/client": "^5.6.0",
    "highlight.js": "^11.9.0",
    "lucia": "^2.7.4",
    "marked": "^11.0.0",
    "marked-base-url": "^1.1.2",
    "marked-highlight": "^2.0.8",
    "marked-katex-extension": "^4.0.5",
    "marked-linkify-it": "^3.1.7",
    "nodemailer": "^6.9.7",
    "svelte-french-toast": "^1.2.0"
  }
}
